{
  "name": "medical-chatbot-backend",
  "version": "1.0.0",
  "description": "AI Chatbot Backend for Medical Students - Basic Health Issues Support",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "seed": "node scripts/seedMedicalKnowledge.js --file ../database/seeds/demo_medical_knowledge.json",
    "import:mesh": "node scripts/importMeSH.js --file ../database/migrations/mesh/desc2024.xml",
    "import:rxterms": "node scripts/importRxTerms.js --file ../database/migrations/rxterms/rxterms.csv",
    "import:csv": "node scripts/importCSV.js --file ../database/seeds/custom_terms.csv"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "dotenv": "^16.3.1",
    "mongoose": "^7.5.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "express-rate-limit": "^6.10.0",
    "joi": "^17.9.2",
    "openai": "^4.0.0",
    "socket.io": "^4.7.2",
    "xml2js": "^0.6.2",
    "csv-parse": "^5.5.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.6.2",
    "supertest": "^6.3.3",
    "eslint": "^8.47.0"
  },
  "keywords": ["medical", "chatbot", "ai", "health", "education"],
  "author": "Medical Education Team",
  "license": "MIT"
}
